# Nagios credentials for integration testing
# Copy this file to .env and fill in your actual values

# Nagios server URL
NAGIOS_URL=http://nagios.example.com/nagios

# Nagios credentials
NAGIOS_USERNAME=nagiosadmin
NAGIOS_PASSWORD=your-password-here

# Optional: Set to 1 to run integration tests
RUN_INTEGRATION_TESTS=0
